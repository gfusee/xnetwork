{
	"index_patterns": [
		"transactions-*"
	],
	"mappings": {
		"properties": {
			"gasLimit": {
				"type": "double"
			},
			"gasPrice": {
				"type": "double"
			},
			"nonce": {
				"type": "double"
			},
			"timestamp": {
				"format": "epoch_second",
				"type": "date"
			}
		}
	},
	"settings": {
		"index": {
			"sort.field": [
				"timestamp",
				"nonce"
			],
			"sort.order": [
				"desc",
				"desc"
			]
		},
		"number_of_replicas": 0,
		"number_of_shards": 5,
		"opendistro.index_state_management.rollover_alias": "transactions"
	}
}
